
<%= render partial: "partials/devise_navbar" %>

<div class="container content page-wrapper pagepad">
  <%= render "partials/flash" %>
  <div class="panel panel-primary">
      <div class="panel-heading">
        <h4 class="panel-title"><i class="fa fa-check-square-o" aria-hidden="true"></i> NEW FEEDBACK</h4>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-xs-6">
            <div class="panel panel-success">
              <div class="panel-heading">
                <h3 class="panel-title">REPORT BEING REVIEWED</h3>
              </div>
              <div class="panel-body">
                 <%= render @report %>
              </div>
            </div>
          </div>
        <div class="col-xs-6">
          <div class="panel panel-primary">
              <div class="panel-heading">
                <h3 class="panel-title">TODAY'S REVIEW/FEEDBACK</h3>
              </div>
              <div class="panel-body">
                <%= render 'form', feedback: @feedback %>
              </div>
            </div>
        </div>
      </div>


      </div>
    </div>
</div>

